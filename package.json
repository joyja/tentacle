{
  "name": "tentacle-edge",
  "version": "0.0.3",
  "description": "A nodejs industrial automation edge gateway with a GraphQL API",
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "dev": "cross-env NODE_ENV=development nodemon ./src/index.js -e js,graphql --watch src",
    "start": "cross-env node ./src/index.js",
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "fix": "eslint --ext .js,.vue --fix --ignore-path .gitignore ."
  },
  "bin": {
    "tentacle": "./src/index.js"
  },
  "main": "src/index.js",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "jest-watch-select-projects": "^1.0.0",
    "jest-watch-typeahead": "^0.4.2",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "@types/jest": "^24.0.25",
    "bcryptjs": "^2.4.3",
    "cross-env": "^6.0.3",
    "date-fns": "^2.8.1",
    "esm": "^3.2.25",
    "ethernet-ip": "^1.2.5",
    "express": "^4.17.1",
    "graphql-request": "^1.8.2",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "modbus-serial": "^7.7.3",
    "mqtt": "^3.0.0",
    "sparkplug-client": "^3.2.1",
    "sparkplug-client-jar": "^3.2.1",
    "sqlite3": "^4.1.0"
